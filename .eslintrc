{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ],
        "object-curly-newline": [
            "warn",
            {
                "multiline": true
            }
        ],
        "func-names": [
            "warn",
            "as-needed"
        ],
        "no-param-reassign": 0
    }
}