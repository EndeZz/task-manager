{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".ts", ".js", ".jsx"] }],
      // "id-length": 0,
      // "indent": [1, 4],
      // "no-console": 0,
      // "no-unused-vars": [1, {"vars": "local", "args": "none"}],
      // "strict": 0
  }
}




