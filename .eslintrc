{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
      "node": true,
      "jasmine": true
  },
  "rules": {
      "id-length": 0,
      "indent": [1, 4],
      "no-console": 0,
      "no-unused-vars": [1, {"vars": "local", "args": "none"}],
      "strict": 0
  }
}




